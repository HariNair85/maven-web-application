pipeline {
    agent any
    
    tools {
        maven 'maven3'
    }

    stages {
      stage('git checkout'){
        steps{
          git 'https://github.com/HariNair85/maven-web-application.git'
        }
      }
      stage('maven build war file'){
        steps{
          sh 'mvn clean package'
        }
      }
    }
}
